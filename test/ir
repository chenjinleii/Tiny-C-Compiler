; ModuleID = 'main'
source_filename = "main"

declare i32 @putchar(i32)

define i32 @fuck(i32 %i) {
entry:
  %i1 = alloca i32
  store i32 0, i32* %i1
  store i32 %i, i32* %i1
  %i2 = load i32, i32* %i1
  ret i32 %i2
}

define i32 @main() {
entry:
  %0 = call i32 @fuck(i32 2)
  %1 = call i32 @putchar(i32 %0)
  ret i32 0
}
